<h1>Add a video</h1>
<ul>
  <% @vimeo_video.errors.full_messages.each do |error| %>
    <li><%= error %></li>
  <% end %>
</ul>


<%= form_for @vimeo_video do |f| %>
  <p>
    <%= f.label :url %>
    <%= f.text_field :url %><br/>
  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_area :description %><br/>
  </p>
  <p>
    <%= f.label :rating %>
    <%= f.text_field :rating %><br/>
  </p>
  <%= f.submit 'Create Video' %>
<% end %>
